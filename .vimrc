syntax on
set relativenumber
set nu rnu
highlight LineNr term=bold ctermfg=DarkGrey guifg=DarkGrey 
set tabstop=4 softtabstop=4
set autoindent
set shiftwidth=4
set expandtab
set wrap
set linebreak
set ignorecase
set smartcase
set incsearch
set showcmd
set undodir=~/.vim/undodir
set undofile
set noswapfile
set title
set cursorline
